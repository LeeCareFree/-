.canvas {
    width: 95%;
    height: 200px;
}

@media (min-width: 480px) {
    /* 仅在 480px 或更宽的屏幕上生效的样式规则 */
    .my-class {
        width: 200px;
    }
}

.flexContainer {
    display: flex;
    justify-content: center;
    align-items: center;
}

.flexContainer .title {
    font-size: calc(36vmin/7.5);
}

.pickerContainer {
    background-color: blanchedalmond;
    border-radius: 50rpx;
    font-size: calc(32vmin/7.5);
    padding: calc(5vmin/7.5) calc(20vmin/7.5);
    margin-left: calc(20vmin/7.5);
    color: gray;
}

.wra {
    width: 100%;
    height: 100%;
}

.match {
    width: 100vw;
    height: 100vh;
}

.line-transverse {
    height: 1px;
    width: 100%;
    background: #e5e5e5;
}

.line-vertical {
    height: 100%;
    width: 1px;
    background: #e5e5e5;
}

.loading-img {
    background: #f5f5f5;
}

.button-next {
    min-width: 30rpx;
    height: 112rpx;
    line-height: 112rpx;
    background: #ed373d;
    border-radius: 10rpx;
    text-align: center;
    vertical-align: middle;
    font-size: 40rpx;
    color: #fff;
}

.button-next-disabled {
    background: #ccc !important;
}

.base-container {
    height: 100%;
    display: flex;
    flex-direction: column;
    padding-left: 36rpx;
    padding-right: 36rpx;
}

.form-box {
    margin-top: 64rpx;
    margin-bottom: 64rpx;
}

.form-item-box {
    height: 110rpx;
    border-bottom: 1rpx solid #ccc;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
}

.form-item-icon {
    width: 24rpx;
    height: 32rpx;
    margin-right: 20rpx;
}

.form-item-input {
    height: 32rpx;
    line-height: 32rpx;
    font-size: 36rpx;
    color: #000;
    flex-grow: 1;
}

.form-item-input-error,
.form-item-input-placeholder-error {
    color: red !important;
}

.form-item-input-clear {
    height: 40rpx;
    width: 40rpx;
}

.form-error-tips-box {
    margin-top: 20rpx;
    color: red;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-content: center;
    height: 36rpx;
}

.form-error-tips-icon {
    width: 18rpx;
    height: 18rpx;
}

.form-error-tips-text {
    margin-left: 30rpx;
    font-size: 16px;
}

.font-size-12 {
    font-size: 24rpx;
}

.font-size-14 {
    font-size: 28rpx;
}

.font-size-16 {
    font-size: 32rpx;
}

.font-size-18 {
    font-size: 36rpx;
}

.font-size-20 {
    font-size: 40rpx;
}

.font-size-24 {
    font-size: 48rpx;
}

.font-size-40 {
    font-size: 80rpx;
}

.index_container {
    display: flex;
    flex-direction: column;
    background-color: #fff;
    margin: 0;
    padding: 0;
}

.index_title_container {
    display: flex;
    flex-direction: row;
    background-color: #fff;
    align-items: center;
    justify-content: space-between;
    margin: 0 32rpx;
    padding: 26rpx 0;
}

.index_title {
    line-height: 44rpx;
    font-size: 32rpx;
    text-align: left;
    color: #1a173b;
    overflow: hidden;
    margin-right: 13rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    flex-shrink: 1;
}

.index_title_icon {
    height: 34rpx;
    width: 34rpx;
    flex-shrink: 0;
}

.index_title_right {
    line-height: 34rpx;
    font-size: 24rpx;
    text-align: right;
    color: #353535;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    flex-grow: 1;
}

.index_line_1 {
    height: 1px;
    margin: 0 32rpx;
    background-color: #dce0e6;
}

.index_line_20 {
    height: 20rpx;
    background-color: #f8f9fa;
}

.index_look_detail {
    width: 100%;
    line-height: 96rpx;
    font-size: 32rpx;
    text-align: center;
    color: #1a173b;
    background-color: #fff;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.index_line_72 {
    height: 72rpx;
    background-color: #f8f9fa;
}

.date_tab {
    margin: 34rpx 32rpx 14rpx;
}

page {
    width: 100%;
    height: 100%;
    background-color: #fff;
}

.swiper-tab {
    display: flex;
    flex-direction: row;
    line-height: calc(80vmin/7.5);
    border-bottom: calc(2vmin/7.5) solid #777;
  }
    
  .tab-item {
    width: 50%;
    text-align: center;
    font-size: calc(32vmin/7.5);
    font-weight: bold;
    color: #777;
  }
    
  .on {
    color: blue;
    border-bottom: calc(5vmin/7.5) solid blue;
  }
.canvasbody{
    display: flex;

}
  .tips{
    position:relative;
    margin-left:calc(20vmin/7.5);
    padding:6rpx;
    color:#fff;
    font-size:calc(26vmin/7.5);
    width:calc(40vmin/7.5);
    height: calc(400vmin/7.5);
    background-color:orange;
    border-radius:4rpx;
}
.tips:after{
    position:absolute;
    left:calc(-40vmin/7.5);
    right:calc(20vmin/7.5);
    top: 20%;
    border-top:calc(20vmin/7.5) solid transparent;
    content:" ";
    width:0;
    height:0;
    border-right:calc(20vmin/7.5) solid orange;
    border-bottom:calc(20vmin/7.5) solid transparent;
    border-left:calc(20vmin/7.5) solid transparent;
}
.ranklist{
    display: flex;
    flex-direction: column;
}

.rank-item{
    display: flex;
    align-items: center;
    padding:calc(20vmin/7.5);
}
.rank-img{
    position: absolute;
    left: 5%;
    width: calc(60vmin/7.5);
    height: calc(60vmin/7.5);
    margin-right: calc(20vmin/7.5);
}

.table {
    border: 0px solid darkgray;
   }
   .tr {
    display: flex;
    width: 100%;
    justify-content: center;
    align-items: center;
    height: calc(80vmin/7.5);
   }
   .td {
    width:40%;
    justify-content: center;
    text-align: center;
    font-size: calc(28vmin/7.5);
   }
   .bg-w{
    background: snow;
   }
   .bg-g{
    background: #E6F3F9;
    border-bottom: 1rpx solid gainsboro;
   }
   .th {
    width: 40%;
    justify-content: center;
    background: #3366FF;
    color: #fff;
    display: flex;
    align-items: center;
    font-size: calc(32vmin/7.5);
    height: calc(80vmin/7.5);
   }